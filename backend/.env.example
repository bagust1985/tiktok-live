# Database
DATABASE_URL="postgresql://tiktok_user:your_secure_password_here@localhost:5432/tiktok_live?schema=public"

# JWT Secret
JWT_SECRET=your_jwt_secret_here_min_32_characters

# Port
PORT=3002

# Node Environment
NODE_ENV=production

# Resend Email Service (for OTP email verification)
RESEND_API_KEY=re_your_resend_api_key_here
FROM_EMAIL=noreply@yourdomain.com

# OTP Configuration (optional - defaults provided)
OTP_EXPIRY_MINUTES=15
OTP_MAX_ATTEMPTS=5
